    },
    "dependencies": {
        "axios": "^0.18",
        "babel-polyfill": "^6.26.0",
        "bootstrap": "^4.2.1",
        "jquery": "^3.3.1",
        "popper.js": "^1.14.6",
        "vue": "^2.5.22",
        "vue-router": "^3.0.2"
    }
}
